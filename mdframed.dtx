\newenvironment{mdframed}[1][]{%
\begingroup%
\color@setgroup%
   \mdfsetup{userdefinedwidth=\linewidth,#1}%
   \mdf@hidealllines@check%
   \mdf@twoside@checklength%
   \let\width\z@%
   \let\height\z@%
   \mdf@checkntheorem%
   \mdf@styledefinition%
